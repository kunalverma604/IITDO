<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:padding="8dp">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="@dimen/list_group_child_icon_width"
        android:layout_height="@dimen/list_group_child_icon_height"
        android:layout_marginLeft="50dp"
        android:tint= "@color/colorPrimaryDark"
        app:srcCompat="@drawable/nav_sub_category" />

    <TextView
        android:id="@+id/lblListItem"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingLeft="16dp"
        android:text="subcategory"
        android:fontFamily="@font/chivo_regular"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="@dimen/list_group_child_text" />

</LinearLayout>
